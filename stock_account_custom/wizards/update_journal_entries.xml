<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
        <record id="wizard_update_journal_entries_view_form" model="ir.ui.view">
        <field name="name">wizard.update.journal.entries.form</field>
        <field name="model">wizard.update.journal.entries</field>
        <field name="arch" type="xml">
            <form string="Update Import">
                <group>
                    <field name="account_move_ids" widget="many2many_tags"/>
                    <!--<field name="journal_id"/>-->
                    <!--<field name="update_from_date"/>-->
                    <!--<field name="update_to_date"/>-->
                </group>
                <footer>
                    <button string="Update" name="update_journal_entries" type="object" class="btn-primary"/>
                    <button string="Cancel" class="btn-secondary" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>

    <!-- <act_window id="wizard_update_journal_entries_view_form_action"
            name="Update Journal Entries"
            src_model="account.move"
            res_model="wizard.update.journal.entries"
            view_type="form" view_mode="form"
            key2="client_action_multi" target="new" groups="base.group_no_one"/> -->

    <record id="wizard_delete_account_account_view_form" model="ir.ui.view">
        <field name="name">wizard.delete.account.account.form</field>
        <field name="model">wizard.delete.account.account</field>
        <field name="arch" type="xml">
            <form string="Clean account">
                <group>
                    <field name="account_ids" widget="many2many_tags"/>
                </group>
                <footer>
                    <button string="Update" name="button_delete_account" type="object" class="btn-primary"/>
                    <button string="Cancel" class="btn-secondary" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>

    <record id="wizard_delete_account_account_view_form_action" model="ir.actions.act_window">
        <field name="name">Clean Account</field>
        <field name="res_model">wizard.delete.account.account</field>
        <field name="view_mode">form</field>
        <field name="target">new</field>
        <field name="context">{}</field>
        <field name="binding_model_id" ref="account.model_account_account"/>
        <field name="binding_view_types">form</field>
        <field name="groups_id" eval="[(4, ref('base.group_no_one'))]"/>
    </record>

    <record id="wizard_fix_wow_cc_reconciliation_view_form" model="ir.ui.view">
        <field name="name">wizard.fix.wow.cc.reconciliation.form</field>
        <field name="model">wizard.fix.wow.cc.reconciliation</field>
        <field name="arch" type="xml">
            <form string="Fix Reconciliation">
                <group>
                    <field name="account_id" required="1"/>
                </group>
                <footer>
                    <button string="Update" name="button_update_reconciliation" type="object" class="btn-primary"/>
                    <button string="Cancel" class="btn-secondary" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>

    <record id="wizard_fix_wow_cc_reconciliation_view_form_action" model="ir.actions.act_window">
        <field name="name">Fix Reconciliation</field>
        <field name="res_model">wizard.fix.wow.cc.reconciliation</field>
        <field name="view_mode">form</field>
        <field name="target">new</field>
        <field name="context">{}</field>
        <field name="binding_model_id" ref="account.model_account_move_line"/>
        <field name="binding_view_types">form</field>
        <field name="groups_id" eval="[(4, ref('base.group_no_one'))]"/>
    </record>

</odoo>
